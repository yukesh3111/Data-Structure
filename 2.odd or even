#include<stdio.h>
int main()
{
	int a[100],n;
	printf("Enter number of element in array : ");
	scanf("%d",&n);
	for(int i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("Even number ");
	for(int i=0;i<n;i++)
	{
		if(a[i]%2==0)
		{
			printf("\n%d ",a[i]);
		}
	}
	printf(" \nOdd number");
	for(int i=0;i<n;i++)
	{
		if(a[i]%2!=0)
		{
			printf("\n %d",a[i]);
		}
	}
}
